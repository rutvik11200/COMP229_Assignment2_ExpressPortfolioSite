<%-include ../partials/header.ejs%>
<%-include ../partials/main_nav.ejs%>
  <!--Main Nav Navigation-->
  
<main class="container">
  <div class="row">
    <div class="offset-md-1 col-md-10">
        <h1><%=title %></h1>
          <%if(typeof displayName =='string'&&displayName){%>
        <%}%>
        <br>
        <table class="table table-bordered table-striped table-hover">
            <!--Header Row-->
            
            <tr>
              <%if(typeof displayName =='string'&&displayName){%>
                <th class="text-center col-2">Name</th>
                <th class="text-center col-2">Number</th>
                <th class="text-center col-1">Email</th>
                <%}else{%>
                  <th class="text-center col-2">Name</th>
                <th class="text-center col-2">Number</th>
                <th class="text-center col-1">Email</th>          
                <%}%>
                </tr>
               <%for(let count = 0; count < ContactList.length; count ++){%>
                    <tr>
                          <%if(typeof displayName =='string'&&displayName){%>
                        <td class="text-center col-2"><%=ContactList[count].name%></td>
                        <td class="text-center col-2"><%=ContactList[count].number%></td>
                        <td class="text-center col-1"><%=ContactList[count].email%></td>
                      
                    
                        <%}else{%>
                          <td class="text-center col-2"><%=ContactList[count].name%></td>
                        <td class="text-center col-2"><%=ContactList[count].number%></td>
                        <td class="text-center col-1"><%=ContactList[count].email%></td>
                    
                    
                        <%}%>  
                      </tr>
                        <%}%>
            </table>
            <a class="btn btn-primary" style="margin-left: 48%; margin-bottom: 10px;" role="button" href="/updateList"><i class="fas fa-pencil-alt"></i>Update</a>
        </div>
    </div>
  </main>
  
   <!--<h1><%= title %></h1>-->
   <!--<p>Welcome to <%= title %></p>-->
   <!--Script section-->
  
<%-include ../partials/bottom_nav.ejs%>
 <%-include ../partials/footer.ejs%>